{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ],
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "module": "esnext",
    "moduleResolution": "node",
    "downlevelIteration": true,
    "noEmit": true,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "shared/*": [
        "./src/shared/*"
      ]
    }
  },
  "include": [
    "./src"
  ]
}